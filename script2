const character = document.getElementById("character");
const container = document.getElementById("container");

let x = container.offs
etWidth / 2 - character.offsetWidth / 2;
let y = container.offsetHeight / 2 - character.offsetHeight / 2;
character.style.left = x + "px";
character.style.top = y + "px";

// User input
document.onkeydown = function(event) {
    switch (event.key) {
      case "w":
        y -= 10;
        character.src = "kelseyback.png";
        break;
      case "a":
        x -= 10;
        character.src = "kelseyleft.png";
        break;
      case "s":
        y += 10;
        character.src = "kelseyfront.png";
        break;
      case "d":
        x += 10;
        character.src = "kelseyright.png";
        break;
    }
    character.style.left = x + "px";
    character.style.top = y + "px";
  };